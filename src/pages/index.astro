---
import Wrapper from "../components/Wrapper.astro";
import { getEntry } from "astro:content";

const welcomeBlurb = await getEntry('welcome_page', 'welcome');
const { Content } = await welcomeBlurb.render();
---

<Wrapper pageTitle={welcomeBlurb.data.pageTitle} headerTitle={welcomeBlurb.data.headerTitle}>
	<Content />
</Wrapper>